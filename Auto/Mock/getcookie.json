[
  {
    "description":"简单的get请求方法",
    "request":{
      "method":"get",
      "uri":"/get"
    },
    "response":{
      "text":"请求接口成功。。。"
    }
  },
  {
    "description":"带参数请求的get接口",
    "request":{
      "uri":"/getparameter",
      "method":"get",
      "queries":{
        "name":"wanglin",
        "age":"18"
      }
    },
    "response":{
      "text":"请求成功.",
      "headers":{
        "Content-Type": "text/html;charset=utf-8"
      }
    }
  },
  {
    "description":"带cookie的请求接口",
    "request":{
      "method":"get",
      "uri":"/getcookie",
      "cookies":{
        "login":"true"
      }
    },
    "response":{
      "text":"请求成功，很快乐吧。。。",
      "headers":{
        "Content-Type": "text/html;charset=utf-8"
      }
    }
  },
  {
    "description":"post请求带cookie",
    "request":{
      "method":"post",
      "uri":"/v1/login",
      "cookies":{
        "login":"true"
      },
      "json":{
        "user":"wanglin",
        "pwsd":"123456"
      }
    },
    "response":{
      "json":{
        "data":{
          "message":"success",
          "status":"200"
        }
      }
    }
  },
  {
    "description":"获取cookie",
    "request":{
      "uri":"/login/cookie",
      "method":"post",
      "headers":{
        "Content-Type":"application/json",
        "AppId":"1000000"
      },
      "json":{
        "user":"wanglin",
        "pwsd":"123456"
      }
    },
    "response":{
      "cookies":{
        "cookie":"999999"
      },
      "json":{
        "message":"success",
        "status":"200"
      },
      "text":"恭喜你获取token成功。"
    }
  },
  {
    "description":"通过cookie去请求接口",
    "request":{
      "uri":"/get/username",
      "method":"get",
      "cookies":{
        "cookie":"999999"
      }
    },
    "response":{
      "json":{
        "text":"获取用户信息成功",
        "message":"success",
        "status":"200"
      }
    }
  }
]