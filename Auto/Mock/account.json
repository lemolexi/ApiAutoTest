  {
    "description": "这是一个带cookies的post请求",
    "request": {
      "uri": "/post/with/cookies",
      "method": "post",
      "cookies": {
        "login": "true"
      },
      "json": {
        "name": "zhangshan",
        "age": "18"
      }
    },
    "response": {
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      },
      "status": 200,
      "json": {
        "zhangshan": "成功",
        "status": 1
      }
    }
  },
  {
